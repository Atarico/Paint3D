   58     {\cf18 int} sizeZ = int(plane3D[0][0].size());\par
   59 \par
   60     {\cf18 int} smallerAxisPixels;\par
   61     {\cf19 if}(width() <= height())\{\par
   62         smallerAxisPixels = width();\par
   63     \}\par
   64     {\cf19 else}\{\par
   65         smallerAxisPixels = height();\par
   66     \}\par
   67     {\cf18 int} biggerAxisVoxels; {\cf20 //the longest axis in relation to our voxel matrix}\par
   68     {\cf19 if}(sizeX >= sizeY)\{\par
   69         biggerAxisVoxels = sizeX;\par
   70     \}\par
   71     {\cf19 else}\{\par
   72         biggerAxisVoxels = sizeY;\par
   73     \}\par
   74     {\cf18 float} normSize = smallerAxisPixels/biggerAxisVoxels; {\cf20 //a relation betwen our smaller pixel axis and our longest voxel axis to be used as a drawing unit.}\par
   75 \par
   76     QPoint *plottingStart = {\cf17 new} QPoint(width()/2 - smallerAxisPixels/2, height()/2 - smallerAxisPixels/2); {\cf20 //defines the 2D point at which we will start painting our voxels.}\par
