   21     \{\par
   22         nx = 0; ny = 0; nz = 0;\par
   23     \}\par
   24 \par
   25     {\cf20 //allocates X}\par
   26     v = {\cf17 new} Voxel**[nx];\par
   27     {\cf20 //verifies if X has been allocated}\par
   28     {\cf19 if}(v == {\cf17 nullptr})\par
   29     \{\par
   30         cout<<{\cf22 "Error allocating the lines of the voxel matrix"}<<endl;\par
   31         exit(0);\par
   32     \}\par
   33 \par
   34     {\cf20 //allocates X's collumns}\par
   35     v[0] = {\cf17 new} Voxel*[nx*ny];\par
   36     {\cf20 //verifies if X[0] has been allocated}\par
   37     {\cf19 if}(v[0] == {\cf17 nullptr})\par
