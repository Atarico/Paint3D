   78 \par
   79     {\cf20 //PAINTING 3D MATRIX}\par
   80     {\cf19 for}({\cf18 int} i=0; i<sizeX; i++)\par
   81     \{\par
   82         {\cf19 for}({\cf18 int} j=0; j<sizeY; j++)\par
   83         \{\par
   84             {\cf19 for}({\cf18 int} k=0; k<sizeZ; k++)\par
   85             \{\par
   86                 {\cf19 if}(!plane3D[i][j][k].isOn)\{\par
   87                     brush.setColor(QColor(240,240,240));\par
   88                     brush.setStyle(Qt::SolidPattern);\par
   89                     p.setBrush(brush);\par
   90 \par
   91                     pen.setColor(QColor(150,150,150));\par
   92                     pen.setWidth(1);\par
   93                     {\cf20 //pen.setStyle(Qt::DashLine);}\par
   94                     p.setPen(pen);\par
   95 \par
   96                     p.drawRect(i*normSize + plottingStart->x(), j*normSize + plottingStart->y(),\par
   97                                normSize, normSize);\par
   98                 \}\par
   99             \}\par
  100         \}\par
  101     \}\par
  102 \}\par
}
