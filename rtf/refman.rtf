   48         cout<<{\cf22 "Error allocating the planes of the voxel matrix"}<<endl;\par
   49         exit(0);\par
   50     \}\par
   51 \par
   52 \par
   53     {\cf20 //indexing X's collumns}\par
   54     {\cf19 for}({\cf18 int} i = 1; i<nx; i++)\{\par
